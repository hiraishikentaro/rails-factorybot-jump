{
  "name": "rails-factorybot-jump",
  "displayName": "Rails FactoryBot Jump",
  "description": "Jump to FactoryBot factory definition from Rails test files",
  "version": "1.2.0",
  "icon": "images/icon.png",
  "publisher": "hir4ken",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hiraishikentaro/rails-factorybot-jump"
  },
  "keywords": [
    "rails",
    "ruby",
    "factorybot",
    "factory",
    "test",
    "testing",
    "rspec",
    "navigation",
    "go to definition",
    "ruby on rails",
    "rails testing",
    "test automation"
  ],
  "categories": [
    "Other",
    "Programming Languages",
    "Testing",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rails-factorybot-jump.jumpToFactory",
        "title": "Jump to FactoryBot Factory"
      }
    ],
    "documentLinkProviders": [
      {
        "scheme": "file",
        "language": "ruby"
      }
    ],
    "configuration": {
      "title": "Rails FactoryBot Jump",
      "properties": {
        "rails-factorybot-jump.factoryPaths": {
          "type": "array",
          "default": [
            "spec/factories/**/*.rb"
          ],
          "description": "Paths to search for factory files. Supports glob patterns.",
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/sinon": "^17.0.1",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^8.56.0",
    "glob": "^8.1.0",
    "mocha": "^10.8.2",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3"
  }
}
